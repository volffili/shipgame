; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "Calendar",
"language": "GDScript",
"path": "res://scripts/ui/calendar.gd"
} ]
_global_script_class_icons={
"Calendar": ""
}

[application]

config/name="ShipGame"
run/main_scene="res://scenes/levels/dock.tscn"

[autoload]

PortRoyal="*res://scripts/state/ports/PortRoyal.gd"
Tortuga="*res://scripts/state/ports/Tortuga.gd"
Margarita="*res://scripts/state/ports/Margarita.gd"
Map="*res://scripts/state/Map.gd"
Ship="*res://scripts/state/Ship.gd"

[display]

window/size/width=640
window/size/height=480
window/size/fullscreen=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[rendering]

vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0, 0, 0, 1 )
